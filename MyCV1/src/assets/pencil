<script type="text/javascript">

    var authOCD = 'True';//function used for password confirmation 
 (function ($) {

        $.fn.ConfirmPassword = function (settings) {
            var running = false;
            //setting default values
            var settings = $.extend({
                successfunction: '',
                downloadtype: ''
            }, settings);
            //show password confirmation popup window
            $('#login').removeClass("hidden").addClass("show").on('shown.bs.modal', function () {
                $("#txtPwd").focus();
                $('#btnFPok').removeClass("show").addClass("hidden");
                $('#FPSuccessDiv').removeClass("show").addClass("hidden");
            }).modal('show');
            //on edit click
            HideResetPassword();
            //registering clicks and other events, after unbinding old
            $("#close").unbind("click").click(function () {
                running = false;
                $("#passwordCon").removeClass("disabled");
                $('#login').modal('hide').removeClass("show").addClass("hidden"); // function close pop up  
                $("#errMsgParentPass").removeClass("has-error");
                $('#errMsgPass').removeClass("show help-block").addClass("hidden");
                $('#txtPwd').val("");
            });

            $("#passwordDiv").unbind("keypress").keypress(function (e) {
                if (e.which || e.keyCode) {
                    if ((e.which == 13) || (e.keyCode == 13)) {
                        if (running === false) {
                            running = true;
                            $("#passwordCon").addClass("disabled"); confirmPasswordAction(); return false;
                        }
                        
                    }
                } else { return true; }
            });

            $("#passwordCon").unbind("click").click(function () {
                $("#passwordCon").addClass("disabled");
                confirmPasswordAction();
            });
    var confirmPasswordAction = function () {
        $("#errMsgParentPass").removeClass("has-error");
        $('#errMsgPass').removeClass("show help-block").addClass("hidden");
        var passwd = $('#txtPwd').val();
        if (passwd.length > 5 && passwd.length < 51) {
            //identify pack download click
            var passwordConfirmData =  {};
            if (downloadtype == "pack") {
                var passwordConfirmData = { "UserId": decodeURI(readCookie("UID")), "Password": passwd };
                $.post(serviceurl + "Commonfunctions.asmx/ConfirmPassword", passwordConfirmData, passwordConfirmSuccess);
            }
            else {
                var passwordConfirmData = { "Password": passwd, "isAuth": authOCD, "Downloadtype": downloadtype };
                $.post("/myalamycustomer.asmx/ConfirmPassword", passwordConfirmData, passwordConfirmSuccess);
            }
        }
        else {
                    running = false;
                    $("#passwordCon").removeClass("disabled");
            $('#NewpasswordMSGDivFP').removeClass("show").addClass("hidden");
            $("#errMsgParentPass").addClass("has-error");
            $('#errMsgPass').html("Sorry, your password's incorrect. Please try again. ").removeClass("hidden").addClass("show help-block");
            $('#txtPwd').val("").focus();
        }
    };//Function call on the success of confirmpassword ajax function
    var passwordConfirmSuccess = function (data, status) {
        if (data.status.code == 200) {
           $("#passwordCon").removeClass("disabled");
            $('#login').modal('hide').removeClass("show").addClass("hidden");
            successfunction();

            if (authOCD == "False") {

                $('#Secretkey').val(data.data);

            }
            //MakeEditable();
        }
        else {
                  running = false;
                    $("#passwordCon").removeClass("disabled");
            $('#NewpasswordMSGDivFP').removeClass("show").addClass("hidden");
            $("#errMsgParentPass").addClass("has-error");
            $('#errMsgPass').html("Sorry, your password's incorrect. Please try again. ").removeClass("hidden").addClass("show help-block");
            $('#txtPwd').val("").focus();
        }
    };
  
            
        };

        $.fn.ConfirmPasswordforBuynow = function (settings) {
            var running = false;
            //show password confirmation popup window
            $('#login').removeClass("hidden").addClass("show").on('shown.bs.modal', function () {
                $("#txtPwd").focus();
            }).modal('show');
            //on edit click
            HideResetPassword();
            //registering clicks and other events, after unbinding old
            $("#close").unbind("click").click(function () {
                running = false;
                $("#passwordCon").removeClass("disabled");
                $('#login').modal('hide').removeClass("show").addClass("hidden"); // function close pop up  
                $("#errMsgParentPass").removeClass("has-error");
                $('#errMsgPass').removeClass("show help-block").addClass("hidden");
                $('#txtPwd').val("");
            });

            $("#passwordDiv").unbind("keypress").keypress(function (e) {
                if (e.which || e.keyCode) {
                    if ((e.which == 13) || (e.keyCode == 13)) {
                        if (running === false) {
                            running = true;
                            $("#passwordCon").addClass("disabled"); confirmPasswordAction(); return false;
                        }
                        
                    }
                } else { return true; }
            });

            $("#passwordCon").unbind("click").click(function () {
                $("#passwordCon").addClass("disabled");
                confirmPasswordAction();
            });
            //forgot password changes


            //function used for password confirmation 
            var confirmPasswordAction = function () {
                $("#errMsgParentPass").removeClass("has-error");
                $('#errMsgPass').removeClass("show help-block").addClass("hidden");
                var passwd = $('#txtPwd').val();
                if (passwd.length > 5 && passwd.length < 51) {
                    var passwordConfirmData = { "Password": passwd };
                    $.post("/myalamycustomer.asmx/ConfirmPasswordforBuynow", passwordConfirmData, passwordConfirmSuccessforBuy);
                }
                else {
                    running = false;
                    $("#passwordCon").removeClass("disabled");
                    $('#NewpasswordMSGDivFP').removeClass("show").addClass("hidden");
                    $("#errMsgParentPass").addClass("has-error");
                    $('#errMsgPass').html("Sorry, your password's incorrect. Please try again. ").removeClass("hidden").addClass("show help-block");
                    $('#txtPwd').val("").focus();
                }
            };

            var passwordConfirmSuccessforBuy = function (data, status) {
                if (data.status.code == 200) {                    
                    $('#Secretuserid').val(data.data);
                    $("#passwordCon").removeClass("disabled");
                    $('#login').modal('hide').removeClass("show").addClass("hidden");
                    successfunction();
                }
                else {
                    running = false;
                    $("#passwordCon").removeClass("disabled");
                    $('#NewpasswordMSGDivFP').removeClass("show").addClass("hidden");
                    $("#errMsgParentPass").addClass("has-error");
                    $('#errMsgPass').html("Sorry, your password's incorrect. Please try again. ").removeClass("hidden").addClass("show help-block");
                    $('#txtPwd').val("").focus();
                }
            };

        };
    }(jQuery));
    (function ($) {
        var FpEmail;
        $.fn.ForgotPassword = function () {
            //$("#ForgotPWTag").unbind("click").click(function () {
            OnForgotPassword();
            //});
                $('#btnFPCancelLogin').unbind("click").click(function () {
                    HideResetPassword();
                });

                $('#btnFPResetPassword').unbind("click").click(function () {
                    ForgotPasswordSubmitConfirm();
                });
                $('#btnFPok').unbind("click").click(function () {
                    $('#login').modal('hide').removeClass("show").addClass("hidden"); // function close pop up  
                    $("#errMsgParentPass").removeClass("has-error");
                    $('#errMsgPass').removeClass("show help-block").addClass("hidden");
                    $('#txtPwd').val("");
                });

                $("#passwordDiv").unbind("keypress").keypress(function (e) { if (e.which || e.keyCode) { if ((e.which == 13) || (e.keyCode == 13)) { ForgotPasswordSubmitConfirm(); return false; } } else { return true; } });

                var ForgotPasswordSubmitConfirm = function () {
                var emailexp = /^[a-zA-Z_0-9][a-zA-Z_0-9\+\.\-\!\'&]{0,}@[a-zA-Z_0-9\.\-\!]+\.[a-zA-Z]{1,}$/i;
                FpEmail = $("#FpEmailM").val();
                if (FpEmail.length > 0) {
                    if (emailexp.test(FpEmail)) {
                        var emailResendData = { "Email": FpEmail, "langcode": "en" };
                        $.post("/myalamycustomer.asmx/ForgotPassword", emailResendData, emailResendSuccess);
                        //window.location.href = Hrefssl + "forgotten-pwd.asp?Email=" + FpEmail + "&returnurl=" + returnHref;
                    } else {
                        $('#LblemailErrorMsgFP').text("Please enter a valid email address.");
                        $("#LblemailErrorMsgFP").removeClass("hidden").addClass("show");
                        $("#LblemailErrorMsgDivFP").addClass("has-error");
                        setTimeout(function () {
                            $("#LblemailErrorMsgFP").removeClass("show").addClass("hidden");
                            $("#LblemailErrorMsgDivFP").removeClass("has-error");
                        }, 5000);
                        $('#FpEmailM').focus();
                    }

                } else {
                    $('#LblemailErrorMsgFP').text("You have not entered any email address.");
                    $("#LblemailErrorMsgFP").removeClass("hidden").addClass("show");
                    $("#LblemailErrorMsgDivFP").addClass("has-error");
                    setTimeout(function () {
                        $("#LblemailErrorMsgFP").removeClass("show").addClass("hidden");
                        $("#LblemailErrorMsgDivFP").removeClass("has-error");
                    }, 5000);
                    $('#FpEmailM').focus();
                }
                };
                var emailResendSuccess = function (data, status) {
                    if (data.status.code == 200) {
                    	$('#FPSuccessDiv').removeClass("hidden").addClass("show");
                        $('#FPDiv').removeClass("show").addClass("hidden");
                        $('#btnFPok').removeClass("hidden").addClass("show");
                        $('#btnFPResetPassword').removeClass("show").addClass("hidden");
                        $('#btnFPCancelLogin').removeClass("show").addClass("hidden");
                        $('#NewpasswordMSGDivFP').removeClass("hidden").addClass("show");
                        $('#SuccessMsg').text("If @@FpEmail is associated with an Alamy account you'll receive an email with instructions on how to reset your password.".replace("@@FpEmail", FpEmail));
                    }
                    else if (data.status.code == 403 && data.status.error_message == "Recentpasswordchange") {
                        $("#passwordDiv").unbind("keypress").keypress(function (e) { if (e.which || e.keyCode) { if ((e.which == 13) || (e.keyCode == 13)) { confirmPasswordAction(); return false; } } else { return true; } });
                        $('#myModalLabelFP').removeClass("show").addClass("hidden");
                        $('#FPDiv').removeClass("show").addClass("hidden");
                        $('#myModalLabelPC').removeClass("hidden").addClass("show");
                        $('#PasswordConfDiv').removeClass("hidden").addClass("show");
                        $('#passwordCon').removeClass("hidden").addClass("show");
                        $('#btnFPResetPassword').removeClass("show").addClass("hidden");
                        $('#btnFPCancelLogin').removeClass("show").addClass("hidden");
                        $('#NewpasswordMSGDivFP').removeClass("hidden").addClass("show");
                        $('#EmailSent').text("A new password has already been emailed to @@FpEmail. It might not arrive straightaway, please wait 30 minutes before requesting another one.".replace("@@FpEmail", FpEmail));
                        $('#txtPwd').val('').focus();
                    }
                    else {
                        $('#LblemailErrorMsgFP').text("Sorry, we don't recognize that email.");
                        $("#LblemailErrorMsgFP").removeClass("hidden").addClass("show");
                        $("#LblemailErrorMsgDivFP").addClass("has-error");
                        $("#FpEmailM").focus();
                    }
                };

        };
    }(jQuery));
    function HideResetPassword()
    {
        $("#passwordDiv").unbind("keypress").keypress(function (e) { if (e.which || e.keyCode) { if ((e.which == 13) || (e.keyCode == 13)) { confirmPasswordAction(); return false; } } else { return true; } });
        $('#myModalLabelFP').removeClass("show").addClass("hidden");
        $('#FPDiv').removeClass("show").addClass("hidden");
        $('#myModalLabelPC').removeClass("hidden").addClass("show");
        $('#PasswordConfDiv').removeClass("hidden").addClass("show");
        $('#passwordCon').removeClass("hidden").addClass("show");
        $('#btnFPResetPassword').removeClass("show").addClass("hidden");
        $('#btnFPCancelLogin').removeClass("show").addClass("hidden");
        $('#NewpasswordMSGDivFP').removeClass("show").addClass("hidden");
        $('#LblemailErrorMsgFP').removeClass("show").addClass("hidden");
        $("#LblemailErrorMsgDivFP").removeClass("has-error");
        $("#errMsgParentPass").removeClass("has-error");
        $('#errMsgPass').removeClass("show help-block").addClass("hidden");
        $('#txtPwd').val('').focus();
    }
    function OnForgotPassword()
    {
        $('#myModalLabelFP').removeClass("hidden").addClass("show");
        $('#FPDiv').removeClass("hidden").addClass("show");
        $('#myModalLabelPC').removeClass("show").addClass("hidden");
        $('#PasswordConfDiv').removeClass("show").addClass("hidden");
        $('#passwordCon').removeClass("show").addClass("hidden");
        $('#btnFPResetPassword').removeClass("hidden").addClass("show");
        $('#btnFPCancelLogin').removeClass("hidden").addClass("show");
        $('#LblemailErrorMsgFP').removeClass("show").addClass("hidden");
        $("#LblemailErrorMsgDivFP").removeClass("has-error");
        $("#errMsgParentPass").removeClass("has-error");
        $('#errMsgPass').removeClass("show help-block").addClass("hidden");
        $("#FpEmailM").val('').focus();
    }

</script>

        

