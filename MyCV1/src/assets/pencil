<script>
	$(document).ready(function(){
		//Purchasepath mobile view touch carousel 
		initailizeCarousel('normal');
		function initailizeCarousel(type){
		    $(".mobileviewnormal .mob-hybrid-google-swipe .keywordlist").touchCarousel({
				wrapper:'.mob-hybrid-google-swipe',
				item:'.mob-keyword-link',
				scrollAmount:350,
				delay:800,
				type: type,
				containerRightMargin:0
			});
		}
		$(window).bind({
			 orientationchange:function(){
				initailizeCarousel('reset');
			}
		});	
	});
</script>
