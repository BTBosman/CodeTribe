<div id="confirmModal" class="modal fade" role="dialog" tabindex="-1" data-backdrop="static" >
  <div class="modal-dialog">

	<!-- Modal content-->
	<div class="modal-content modal-tab-content">
	  <div class="modal-header modal-tab-header">
			<i aria-hidden="true" data-dismiss="modal" class="pull-right icon-close h4 modal-tab-close"></i>
			<div id="AlertTitle" class="modal-title modal-tab-title col-md-4 col-sm-4 col-xs-6">Confirm</div>
		</div>
		<i class="icon-arrow-down modal-tab-arrow  col-md-4  col-sm-4 col-xs-6 h6"></i>
