    $(document).ready(function () {
        var type = 3;
        var freeImageContributorType = 0;
        //For same photographer images request, searchkeyword(qt) is a must
        if(((type == 1 || type == 3) && compRequestParameters.searchKeyword == '') || (type == 2 && (imageHeader.queryTerms =='' || !(compRequestParameters.imageRef) ))) return;
        if (configSetting.isIntranet){
            $("#hyb-loader").insertBefore(".categorieslist")
        }
        $("#hyb-loader").css("display", "block");       
        InitSimilars(type,freeImageContributorType);
    });   
