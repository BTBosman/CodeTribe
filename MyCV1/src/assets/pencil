      <script type="text/javascript">
          $(window).load(function () {
              // Measure a view of product details. This shows the detail view occurs on pageload,
              // and also tracks a standard pageview of the details page.
              var imageType = (imageHeader.imageType == 1) ? 'flipped' : 'original';
              var imgref = (imageHeader.imageType == 1) ? imageHeader.flipImgRef : imageHeader.imageRef;
              if (imageHeader.getpicturetype() == "Images") {
                  dataLayer.push({
                      'event': 'detailview',
                      'ecommerce': {
                          'detail': {
                        
                              'products': [{
                                  "name": "",
                                  "id":imgref,
                                  'category': imageHeader.getpicturetype(),                      
                                  "quantity":1,
                                  'dimension14': imageType     
                              }]
                          }
                      }
                  });
              } else {
                  dataLayer.push({
                      'event': 'detailview',
                      'ecommerce': {
                          'detail': {
                        
                              'products': [{
                                  "name": "",
                                  "id":imgref,
                                  'category': imageHeader.getpicturetype(),                      
                                  "quantity":1
                              }]
                          }
                      }
                  });
              }
  
          });

          $(document).ready(function () {

              if (IsMobiledevice == "1" && IsPopularprice == "1"){
             
              window.location.href = requestHeader.href + "mediacomp/pricecalc.aspx?ref=" + imageHeader.imageRef + "&imageid=" + imageHeader.imageId;
             }else
				$(".solid-loader,.solid-loader-bg").hide();
          });
    </script>
